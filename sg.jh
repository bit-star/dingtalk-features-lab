



entity PublicData {
	name String,
    fee BigDecimal,
    reason String,
    itemType ItemType,
    typesOfFee TypesOfFee,
    agree Boolean
}

entity PrivateData {
	name String,
    fee BigDecimal,
    reason String,
    itemType ItemType,
    typesOfFee TypesOfFee,
    agree Boolean
}

enum ItemType{
	CostBudget,
    ContractAmount
}

enum TypesOfFee{
		IT , Purchase , DueDiligence
}


relationship  OneToMany {
	PublicData to PrivateData
}

